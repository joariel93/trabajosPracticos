#ifndef UTN_OPERACIONES_
#define UTN_OPERACIONES_
int utn_pideDosNumeros(int *numberOne,int *numberTwo);
int utn_sumar(int numberOne,int numberTwo,int *resultSum);
int utn_restar(int numberOne,int numberTwo,int *resultSub);
